{
    "name": "brillante",
    "version": "2.0.0",
    "scripts": {
        "ng": "ng",
        "start": "node server.js",
        "build": "ng build --aot --configuration production",
        "test": "ng test",
        "format:fix": "pretty-quick --staged",
        "precommit": "run-s format:fix lint",
        "lint": "ng lint",
        "format:check": "prettier --config prettier.config.js --list-different \\\"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\\\"",
        "e2e": "ng e2e",
        "serve": "ng serve --ssl --ssl-key ./src/assets/certificates/localhost.key  --ssl-cert ./src/assets/certificates/localhost.crt",
        "serve-aot": "ng serve --aot --ssl --ssl-key ./src/assets/certificates/localhost.key  --ssl-cert ./src/assets/certificates/localhost.crt",
        "build-serve": "npm run-script build && npm run-script start",
        "build-prod": "npm run-script build --configuration production --aot && npm run-script start",
        "sanity-start": "cd shop-db && sanity start"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^12.2.12",
        "@angular/common": "~12.2.12",
        "@angular/compiler": "~12.2.12",
