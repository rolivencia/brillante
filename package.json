{
    "name": "brillante",
    "version": "2.0.0",
    "engines": {
        "node": "22.x"
    },
    "scripts": {
        "ng": "ng",
        "start": "node server.js",
        "build": "ng build --aot --configuration production",
        "dev": "concurrently -p=\"{name}\" -n=\"NodeJS,Angular\" -c=\"red,green\" \"npm run serve:be\" \"npm run serve:fe\"",
        "test": "ng test",
        "format:fix": "pretty-quick --staged",
        "precommit": "run-s format:fix",
        "lint": "ng lint",
        "format:check": "prettier --config prettier.config.js --list-different \\\"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\\\"",
        "serve:fe": "ng serve --ssl --ssl-key ./src/assets/certificates/localhost.key  --ssl-cert ./src/assets/certificates/localhost.crt",
        "serve:be": "nodemon --env-file=.env server.js",
        "build-serve": "npm run-script build && npm run-script start",
        "build-prod": "npm run-script build --configuration production --aot && npm run-script start"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^20.1.6",
        "@angular/common": "^20.1.6",
        "@angular/compiler": "^20.1.6",
        "@angular/core": "^20.1.6",
        "@angular/forms": "^20.1.6",
        "@angular/localize": "^20.1.6",
        "@angular/platform-browser": "^20.1.6",
        "@angular/platform-browser-dynamic": "^20.1.6",
        "@angular/router": "^20.1.6",
        "@auth0/auth0-angular": "^2.2.3",
        "@fortawesome/angular-fontawesome": "^3.0.0",
        "@fortawesome/fontawesome-svg-core": "^7.0.0",
        "@fortawesome/free-brands-svg-icons": "^7.0.0",
        "@fortawesome/free-solid-svg-icons": "^7.0.0",
        "@ng-bootstrap/ng-bootstrap": "19.0.1",
        "@popperjs/core": "2.11.8",
        "@syncfusion/ej2-angular-base": "^30.2.4",
        "@syncfusion/ej2-angular-buttons": "^30.2.4",
        "@syncfusion/ej2-angular-calendars": "^30.2.4",
        "@syncfusion/ej2-angular-dropdowns": "^30.2.5",
        "@syncfusion/ej2-angular-grids": "^30.2.5",
        "@syncfusion/ej2-angular-inputs": "^30.2.4",
        "@syncfusion/ej2-angular-lists": "^30.2.4",
        "@syncfusion/ej2-angular-navigations": "^30.2.5",
        "@syncfusion/ej2-data": "^30.2.4",
        "@syncfusion/ej2-icons": "^30.2.4",
        "@syncfusion/ej2-inputs": "^30.2.4",
        "@types/jszip": "^3.1.6",
        "ajv": "^6.12.0",
        "ajv-keywords": "^3.4.1",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
        "bootstrap": "5.3.6",
        "core-js": "^2.6.11",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "express": "^4.17.1",
        "express-jwt": "^5.3.3",
        "jsonwebtoken": "^8.5.1",
        "jspdf": "^2.4.0",
        "jszip": "^2.5.0",
        "lodash": "^4.17.20",
        "mysql2": "^2.3.3",
        "ngx-device-detector": "^10.0.2",
        "ngx-toastr": "^19.0.0",
        "rootpath": "^0.1.2",
        "rxjs": "~6.5.5",
        "sequelize": "^6.21.3",
        "tslib": "^2.0.0",
        "zone.js": "~0.15.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^20.1.5",
        "@angular/cli": "^20.1.5",
        "@angular/compiler-cli": "^20.1.6",
        "@angular/language-service": "^20.1.6",
        "concurrently": "^7.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-angular": "^4.0.1",
        "eslint-plugin-prettier": "^3.1.2",
        "gzipper": "^2.10.4",
        "husky": "^3.1.0",
        "nodemon": "^3.1.10",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "pretty-quick": "^1.11.1",
        "sequelize-cli": "^6.4.1",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "~5.8.3"
    }
}
