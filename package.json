        "@sanity/client": "^2.19.0",
        "@sanity/image-url": "^1.0.1",
        "@syncfusion/ej2-angular-lists": "^19.3.45",
        "@syncfusion/ej2-angular-navigations": "^19.3.46",
        "@types/jszip": "^3.1.6",
        "ajv": "^6.12.0",
        "ajv-keywords": "^3.4.1",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.19.0",
