        "@fortawesome/free-solid-svg-icons": "^6.0.0",
        "@ng-bootstrap/ng-bootstrap": "^12.0.1",
        "@popperjs/core": "^2.10.2",
        "@sanity/client": "^2.19.0",
        "@sanity/image-url": "^1.0.1",
        "@syncfusion/ej2-angular-base": "^19.4.52",
        "@syncfusion/ej2-angular-buttons": "^19.4.48",
        "@syncfusion/ej2-angular-calendars": "^19.4.48",
